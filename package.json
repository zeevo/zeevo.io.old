{
  "name": "zeevo.io",
  "version": "3.0.0",
  "description": "zeevo.io",
  "keywords": [
    "gatsby",
    "blog",
    "react",
    "theme",
    "starter"
  ],
  "repository": "git+https://github.com/zeevo/zeevo.io",
  "license": "MIT",
  "author": "Shane O'Neill",
  "main": "n/a",
  "scripts": {
    "build": "gatsby build",
    "checkin": "node scripts/checkin.js",
    "clean": "rm -rf public .cache",
    "deploy": "npm run build && npm run deploy-s3",
    "deploy-s3": "gatsby-plugin-s3 deploy -y",
    "develop": "gatsby develop",
    "format": "prettier --write './*.js' 'src/**/*.js' 'content'",
    "lint": "./node_modules/.bin/eslint --cache --ext .js,.jsx --ignore-pattern public .",
    "prettier": "prettier --check './*.js' 'src/**/*.js'",
    "serve": "gatsby serve",
    "test": "echo \"Error: no test specified\" && exit 1",
    "upgrade-packages": "npx -p npm-check-updates ncu -u"
  },
  "dependencies": {
    "@headlessui/react": "^1.4.2",
    "@nivo/calendar": "^0.74.0",
    "@nivo/core": "^0.74.0",
    "@nivo/tooltip": "^0.74.0",
    "@popperjs/core": "^2.10.2",
    "gatsby": "^4.1.3",
    "gatsby-plugin-catch-links": "^4.1.0",
    "gatsby-plugin-feed": "^4.1.1",
    "gatsby-plugin-google-analytics": "^4.1.0",
    "gatsby-plugin-image": "^2.1.2",
    "gatsby-plugin-manifest": "^4.1.3",
    "gatsby-plugin-react-helmet": "^5.1.0",
    "gatsby-plugin-sass": "^5.1.1",
    "gatsby-plugin-sharp": "^4.1.3",
    "gatsby-plugin-sitemap": "^5.1.1",
    "gatsby-plugin-theme-ui": "^0.12.0",
    "gatsby-remark-autolink-headers": "^5.1.1",
    "gatsby-remark-copy-linked-files": "^5.1.0",
    "gatsby-remark-images": "^6.1.3",
    "gatsby-remark-prismjs": "^6.1.0",
    "gatsby-remark-responsive-iframe": "^5.1.0",
    "gatsby-remark-table-of-contents": "^1.0.0",
    "gatsby-source-filesystem": "^4.1.2",
    "gatsby-transformer-json": "^4.1.0",
    "gatsby-transformer-remark": "^5.1.3",
    "gatsby-transformer-sharp": "^4.1.0",
    "lodash": "^4.17.21",
    "node-sass": "^6.0.1",
    "prismjs": "^1.25.0",
    "react": "^17.0.1",
    "react-burger-menu": "^3.0.6",
    "react-dom": "^17.0.1",
    "react-helmet": "^6.1.0",
    "react-popper": "^2.2.5",
    "tailwindcss": "^2.2.19",
    "theme-ui": "^0.12.0"
  },
  "devDependencies": {
    "prettier": "^2.4.1"
  },
  "bugs": {
    "url": "https://github.com/gatsbyjs/gatsby/issues"
  }
}
